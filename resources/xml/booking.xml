<?xml version='1.0' encoding='utf-8'?>
<ns2:Booking xmlns:ns2="http://api.paxport.se/openpax/messages" xmlns="http://api.paxport.se/openpax/types">
    <BookingNumber>${bono}</BookingNumber>
    <CarrierCode>${prefix}</CarrierCode>
    <TourOperatorName>Test</TourOperatorName>
    <TourOperatorCode>TTT</TourOperatorCode>
    <FlightList>
        <!--1 or more repetitions:-->
        <Flight>
            <FlightId>
                <CarrierCode>${prefix}</CarrierCode>
                <FlightNumber>${flno}</FlightNumber>
                <DepartureDateTime>${departureDate}</DepartureDateTime>
            </FlightId>
            <DepartureAirport>
                <Code>${departureAirport}</Code>
            </DepartureAirport>
            <ArrivalTime>${arrivalDate}</ArrivalTime>
            <ArrivalAirport>
                <Code>${arrivalAirport}</Code>
            </ArrivalAirport>
            <CabinClass>${serviceType}</CabinClass>
        </Flight>
    </FlightList>
    <PassengerList>
        <Passenger>
            <FirstName>${passengerName}</FirstName>
            <LastName>${passengerSurname}</LastName>
            <Category>adult</Category>
            <Age>30</Age>
            <Gender>${sex}</Gender>
        </Passenger>
    </PassengerList>
</ns2:Booking>
